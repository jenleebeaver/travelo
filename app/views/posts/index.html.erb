

<div class="container">
<header></header>
<main>
  <div class="main-header">
      <h1 class="h1-main"> Hey <%= current_user.full_name %>!<br/></h1>
      <h2 class="h2-main">Here are all of your posts.</h2> 
  </div>

<table>
  <thead>
    <tr>
      <th><%= %></th>
    </tr>
  </thead>

  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= post.content %></td>
        <td>
          <%= link_to 'Edit', edit_post_path(post) %> |
          <%= link_to 'Delete', post, method: :delete, data: { confirm: 'Are you sure you want to delete this post?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
#insert comment link here 

<div>
    <a href='/posts/new'>Create New Post</a>
</div>

 

</main>
</div>